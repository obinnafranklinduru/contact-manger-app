{
  "name": "contact-manger-app",
  "version": "1.0.0",
  "description": "This is a Contact Manager API built with Node.js that demonstrates CRUD operations on a database and utilizes JSON Web Tokens (JWT) for user authentication.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "watch": "nodemon server.js",
    "test": "jest --detectOpenHandles",
    "test-watch": "jest --watch",
    "pm2-start": "pm2 start server.js --name contact-manger-app --watch --instances max",
    "pm2-deploy": "pm2 start server.js --name contact-manger-app --watch --env production --instances max", 
    "pm2-stop": "pm2 stop server.js",
    "pm2-restart": "pm2 restart server.js",
    "pm2-delete": "pm2 delete server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obinnafranklinduru/contact-manger-app.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/obinnafranklinduru/contact-manger-app/issues"
  },
  "homepage": "https://github.com/obinnafranklinduru/contact-manger-app#readme",
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 1000000
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.1.1",
    "morgan": "^1.10.0",
    "pm2": "^5.3.0",
    "validator": "^13.9.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  }
}
